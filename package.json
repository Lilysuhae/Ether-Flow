{
  "name": "ether-flow",
  "version": "0.5.0",
  "description": "Alchemy Productivity App",
  "author": "Lilysuhae",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "rebuild": "electron-rebuild -v 40.0.0",
    "build": "npm run rebuild && electron-builder --win --x64"
  },
  "build": {
  "appId": "com.lilysuhae.etherflow",
  "productName": "Ether Flow",
  "directories": {
    "output": "demo"
  },
  "asar": true,
  "asarUnpack": [
    "**/node_modules/active-win/**/*"
  ],
  "files": [
    "**/*",
    "!node_modules",
    "node_modules/active-win/**/*",
    "!demo", 
    "!.github"
  ],
  "win": {
    "target": "nsis",
    "icon": "assets/icons/icon.ico"
  }
  },
  "dependencies": {
    "active-win": "^9.0.0"
  },
  "devDependencies": {
    "electron": "^40.0.0",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9"
  }
}