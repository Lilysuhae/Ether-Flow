{
  "name": "ether-flow",
  "version": "0.5.6",
  "description": "Alchemy Productivity App",
  "author": "Lilysuhae",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "rebuild": "electron-rebuild -v 40.0.0",
    "build": "npm run rebuild && electron-builder --win --x64"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lilysuhae/Ether-Flow.git"
  },
  "build": {
    "appId": "com.lilysuhae.etherflow",
    "productName": "Ether Flow",
    "directories": {
      "output": "demo"
    },
    "asar": true,
    "asarUnpack": [
      "**/node_modules/active-win/**/*",
      "**/node_modules/@mapbox/node-pre-gyp/**/*"
    ],
    "files": [
      "**/*",
      "!demo",
      "!node_modules/*/{test,tests,example,examples,docs}",
      "!.github"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icons/icon.ico"
    }
  },
  "dependencies": {
    "@mapbox/node-pre-gyp": "^2.0.3",
    "active-win": "^9.0.0",
    "axios": "^1.13.2",
    "nopt": "^9.0.0"
  },
  "devDependencies": {
    "electron": "^40.0.0",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9"
  }
}
